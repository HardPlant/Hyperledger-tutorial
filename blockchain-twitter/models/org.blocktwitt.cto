namespace org.blocktwitt

asset Tweet identified by tweetId{
  o String tweetId
  o String Message
  --> User user
}

participant User identified by userId {
  o String userId
  o String firstName
  o String lastName
}

transaction readTweet {
  o String twittId
}
transaction writeTweet {
  --> User user
  o String message
}